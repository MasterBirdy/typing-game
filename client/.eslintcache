ionState) => state.status);\n    const { id } = statusState;\n    const usersOnline = Object.keys(users).filter((user) => users[user].status === Status.IDLE && user !== id);\n    const usersBusy = Object.keys(users).filter((user) => users[user].status !== Status.IDLE && user !== id);\n    return (\n        <Container marginTop={1}>\n            <UsersHeader>\n                <GoPrimitiveDot />\n                Users Online:\n            </UsersHeader>\n            <Grid columns={12}>\n                {usersOnline.length ? (\n                    usersOnline.map((user) => (\n                        <GridItem span={3} key={user}>\n                            <PlayerCard\n                                clickHandler={() => context?.challenge(users[user])}\n                                user={users[user].name}\n                                status={users[user].status}\n                            />\n                        </GridItem>\n                    ))\n                ) : (\n                    <GridItem span={12}>\n                        <p>No users here!</p>\n                    </GridItem>\n                )}\n            </Grid>\n            <UsersHeader className=\"users-busy\" marginTop={1.25}>\n                <GoPrimitiveDot />\n                Users Busy:\n            </UsersHeader>\n            <Grid columns={12}>\n                {usersBusy.length ? (\n                    usersBusy.map((user) => (\n                        <GridItem span={3} key={user}>\n                            <PlayerCard\n                                clickHandler={() => context?.challenge(users[user])}\n                                user={users[user].name}\n                                status={users[user].status}\n                            />\n                        </GridItem>\n                    ))\n                ) : (\n                    <GridItem span={12}>\n                        <p>No users here!</p>\n                    </GridItem>\n                )}\n